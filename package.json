{
  "name": "raternet",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:7000",
  "scripts": {
    "client": "cd Client && npm install && npm start",
    "server": "cd Server && npm install && node app.js",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
