    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@react-google-maps/api": "^2.20.6",
    "mongodb": "^6.13.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
